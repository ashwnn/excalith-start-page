{
	"username": "ashwnn",
	"title": "Start Page",
	"theme": {
		"name": "Blue",
		"backgroundColor": "#0f1419",
		"windowColor": "#1a1f2e",
		"textColor": "#e2e8f0",
		"black": "#0f172a",
		"red": "#ef4444",
		"green": "#10b981",
		"yellow": "#f59e0b",
		"blue": "#3B82F6",
		"magenta": "#8b5cf6",
		"cyan": "#06b6d4",
		"white": "#f1f5f9",
		"gray": "#94a3b8"
	},
	"wallpaper": {
		"url": "assets/wallpaper.png",
		"easing": "ease-in-out",
		"fadeIn": true,
		"blur": true
	},
	"terminal": {
		"fixedHeight": true,
		"windowGlow": true,
		"windowGlowColor": "#3B82F620",
		"textGlow": false,
		"opacity": 0,
		"blur": 16
	},
	"prompt": {
		"ctrlC": true,
		"placeholder": "command...",
		"placeholderColor": "gray",
		"userColor": "blue",
		"atColor": "gray",
		"hostColor": "cyan",
		"promptColor": "blue",
		"promptSymbol": "‚ùØ",
		"caretColor": "blue",
		"selectionBg": "blue",
		"selectionFg": "white"
	},
	"fetch": {
		"timeFormat": "HH:mm",
		"dateFormat": "DD/MM/YYYY",
		"titleColor": "blue",
		"image": "assets/icon.png",
		"data": [
			"Time: {time}",
			"Date: {date}",
			"{seperator}",
			"OS: {osName} {osVersion}",
			"Theme: {theme}",
			"{seperator}",
			"Browser: {browser} {browserVersion}",
			"Engine: {engineName}",
			"{seperator}",
			"{colors}"
		]
	},
	"urlLaunch": {
		"target": "_blank",
		"hoverColor": "blue"
	},
	"search": {
		"default": "https://duck.com/search?q={}",
		"target": "_blank",
		"shortcutRegex": "(!?[A-Za-z0-9]+) (.*)",
		"shortcuts": [
			{
				"alias": "g",
				"name": "Google Search",
				"url": "https://google.com/search?q={}"
			},
			{
				"alias": "d",
				"name": "DuckDuckGo Search",
				"url": "https://duckduckgo.com/?q={}"
			},
			{
				"alias": "!d",
				"name": "DuckDuckGo Lucky Search",
				"url": "https://duckduckgo.com/?q=!ducky+{}"
			},
			{
				"alias": "gh",
				"name": "Github Search",
				"url": "https://github.com/search?q={}"
			},
			{
				"alias": "s",
				"name": "Stack Overflow Search",
				"url": "https://stackoverflow.com/search?q={}"
			}
		]
	},
